{
  "name": "telegram-vpn-bot",
  "version": "1.0.0",
  "description": "Telegram VPN Subscription Bot (Phase 1 MVP)",
  "main": "dist/src/index.js",
  "scripts": {
    "start": "node dist/src/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "migrate:dev": "prisma migrate dev --name init",
    "migrate": "prisma migrate deploy",
    "prisma:generate": "prisma generate",
    "seed": "ts-node scripts/seed-db.ts"
    },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@grammyjs/storage-redis": "^2.5.1",
    "@prisma/client": "^5.20.0",
    "bullmq": "^5.0.0",
    "dotenv": "^16.4.0",
    "express": "^4.19.0",
    "grammy": "^1.26.0",
    "ioredis": "^5.3.0",
    "opossum": "^8.0.0",
    "pino": "^9.0.0",
    "pino-pretty": "^11.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.0.0",
    "prisma": "^5.20.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.0"
  }
}
